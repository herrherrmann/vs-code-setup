[{
    "key": "alt+cmd+b",
    "command": "HookyQR.beautify",
    "when": "editorTextFocus"
}, {
    "key": "ctrl+shift+l",
    "command": "workbench.action.editor.changeLanguageMode",
    "when": "editorTextFocus"
}, {
    "key": "ctrl+shift+l",
    "command": "editor.action.deleteLines",
    "when": "editorTextFocus"
}]

